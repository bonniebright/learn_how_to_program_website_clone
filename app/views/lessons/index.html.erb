<h1> Table of Contents </h1>
<ul>
  <% Chapter.all.each do |chapter| %>
    <li> <%= chapter.name %>
    <ul class="archive_section">
    <% chapter.sections.each do |section| %>
      <li class="sections"><%= section.name %>
      <ul class="archive_lessons">
        <% section.lessons.each do |lesson| %>
          <li class="lessons"> <a href="/lessons/<%=lesson.id%>"><%= lesson.name %> </a> </li>
        <% end %>
      </ul>
      </li>
    <% end %>
    </ul>
    </li>
  <% end %>
</ul>







